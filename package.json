{
  "name": "bennibot",
  "version": "4.5.0",
  "description": "bennibot is a music & multipurpose discord bot written using discordjs",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "update": "npm update && npm upgrade && npx depcheck",
    "dockerImage": "docker build . -t benniblot/bennibot --pull --platform linux/arm64 && docker image save benniblot/bennibot --output bennibot.tar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benniblot/bennibot.git"
  },
  "keywords": [
    "typescript",
    "discord",
    "discord-js",
    "discordbot",
    "musicbotdiscord"
  ],
  "author": "benniblot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benniblot/bennibot/issues"
  },
  "homepage": "https://github.com/benniblot/bennibot#readme",
  "dependencies": {
    "@discordjs/builders": "^1.4.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^1.5.0",
    "@discordjs/voice": "^0.14.0",
    "axios": "^1.4.0",
    "colors": "^1.4.0",
    "discord-api-types": "^0.37.46",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "ffmpeg-static": "^5.1.0",
    "libsodium-wrappers": "^0.7.11",
    "node-fetch": "^3.3.1",
    "tweetnacl": "^1.0.3",
    "yt-search": "^2.10.4",
    "ytdl-core": "^4.11.4",
    "ytdl-core-discord": "^1.3.1"
  },
  "devDependencies": {
    "@types/yt-search": "^2.3.2",
    "typescript": "^5.1.3"
  }
}
